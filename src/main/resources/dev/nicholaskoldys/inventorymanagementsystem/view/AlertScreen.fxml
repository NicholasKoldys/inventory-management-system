<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox 
xmlns:fx="http://javafx.com/fxml/1" 
xmlns="http://javafx.com/javafx/8.0.221"
fx:controller="dev.nicholaskoldys.inventorymanagementsystem.controller.AlertSceneController">
    <children>
        <Label text="You are about to delete an Object"
        fx:id="messageLabel" />
        <Label text="Are you sure" />
        <HBox>
            <children> 
                <Button text="Yes, I'm sure"
                fx:id="yesButton"
                onAction="#yesButtonActionMainScreen" />
                <Button text="No"
                fx:id="noButton"
                onAction="#noButtonActionGoBack" />
            </children>
        </HBox>
    </children>
</VBox>